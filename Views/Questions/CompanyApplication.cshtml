                                            <td><a href="#" data-toggle="modal" class="btn btn-sm btn-icon btn-secondary" id="@subs._id" name="edit"><i class="fa fa-pencil-alt"></i> <span class="sr-only">Edit</span></a> <a href="#" class="btn btn-sm btn-icon btn-secondary"><i class="far fa-trash-alt"></i> <span class="sr-only">Remove</span></a></td>

                                        </tr>
}

                                }}
                                                                </tbody><!-- /tbody -->
                                                            </table><!-- /.table -->
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <!-- .card -->
                                            <!-- .tab-pane -->


                                        </div>
                                        @*end here*@
                                    </form>
                                
                                        <!-- /.table-responsive -->
</div>
                            </div>
