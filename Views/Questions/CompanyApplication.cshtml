                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="#physicalAddress">Physical address</label>
                                        <input type="text" data-minlength="6" class="form-control" id="physicalAddress" name="physicalAddress" value="@ViewBag.nameOffice.Office.PhysicalAddress" required>

                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="#postalAddress">Postal address</label>
                                        <input type="text" class="form-control" id="postalAddress" name="postalAddress" value="@ViewBag.nameOffice.Office.PostalAddress" required>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="#city">City</label>
                                        <select class="form-control" id="city" name="city" value="@ViewBag.nameOffice.Office.City" required>
                                            <option value="Harare" selected>Harare</option>
                                            <option value="Mutare">Mutare</option>
                                            <option value="Kadoma">Kadoma</option>
                                            <option value="Gweru">Gweru</option>
                                            <option value="Kwekwe">Kwekwe</option>
                                        </select>
                                        @*<label>City</label>
                                            <input type="text" class="form-control" id="city" required>*@
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="#emailAddress">Email address</label>
                                        <input type="email" class="form-control" id="emailAddress" name="emailAddress" value="@ViewBag.nameOffice.Office.EmailAddress " required>
                                    </div>
                                </div>
                            </div>

                            <div class="row">

                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="#telephone">Telephone number</label>
                                        <input type="text" class="form-control" id="telephone" name="telephone" value="@ViewBag.nameOffice.Office.TelephoneNumber" required>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label>Mobile number</label>
                                        <input type="text" class="form-control" id="mobileNumber" name="mobileNumber" value="@ViewBag.nameOffice.Office.MobileNumber" required>
                                    </div>
                                </div>
                            </div>

                            <div class="row">

                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="#EffectiveDate">Effective  Date</label>
                                        <input type="date" class="form-control" id="EffectiveDate" name="EffectiveDate" value="@ViewBag.nameOffice.Office.EffectiveDate" required>
                                    </div>
                                </div>

                            </div>

                            <div class="form-group fa-pull-right mt-5">
                                <button type="submit" id="submitOffice">
                                    Save Office Details
                                </button>
                            </div>
                        </div>

                    </div>
                </form>
            </div>






