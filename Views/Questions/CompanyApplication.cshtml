                                        <!-- .card-header -->
                                        <div id="addshare" class="card-header  d-flex">
                                            <!-- .dropdown -->

                                            <a href="#addEmployeeModal" id="add" class="btn btn-success btn-primary ml-auto" data-toggle="modal">Add Shareholder</a>
                                        </div><!-- /.card-header -->
                                        <!-- .table-responsive -->
                                        <div class="table-responsive">
                                            <!-- .table -->
                                            <table class="table">
                                                <!-- thead -->
                                                <thead>
                                                    <tr>
                                                        <th style="min-width:256px"> Full Name </th>
                                                        <th>National ID/Passport</th>
                                                        <th>Nationality </th>
                                                        <th> Address </th>
                                                        <th> Date of Appoinment </th>
                                                        <th>Representing </th>
                                                        <th>Action</th>
                                                    </tr>
                                                </thead><!-- /thead -->
                                                <!-- tbody -->
                                                <tbody id="shareholders">
                                                    <!-- tr -->     @if (ViewBag.Subscribers != null)
                                                    {
                                        @foreach (var subs in ViewBag.Subscribers)
                                        {
                                            if (subs.type == "Self")
                                            {


                        <tr>


                            <td>@subs.surname  @subs.names </td>

                            <td>@subs.id_no</td>

                            <td>@subs.nationality</td>

                            <td>@subs.address</td>
                            <td>@subs.names</td>

