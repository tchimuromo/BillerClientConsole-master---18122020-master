                                                    <div class="card card-fluid">
                                                        <!-- #summernote-basic -->
                                                        <textarea type="text" id="liabilityClause" class="form-control" rows="5" name="liab" required>@ViewBag.liability</textarea>

                                                    </div><!-- /.card-body -->

                                                </div><!-- /.card -->
